# Server
PORT=4000

# Firebase
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_CLIENT_EMAIL=your-client-email@your-project.iam.gserviceaccount.com
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n"
FIREBASE_API_KEY=your-web-api-key

# Judge0
JUDGE0_URL=https://judge0-ce.p.rapidapi.com
JUDGE0_KEY=your-rapidapi-key

# Supabase (Storage for certificates)
SUPABASE_URL=https://your-supabase-project.supabase.co
SUPABASE_KEY=service_role_or_secret_key
SUPABASE_CERT_BUCKET=certificates

# Internal secret for internal triggers
INTERNAL_SECRET=some-secret-value

# Optional
NODE_ENV=development
